<%- include('./partials/top')%>

<body class="theme-red">


    <%- include('./partials/topBar')%>
    <%- include('./partials/navBar')%>

    <section class="content">
        <div class="container-fluid">
            <!-- Basic Examples -->
            <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="card">
                        <div class="header">
                            <h2>
                                ALL SERVICES
                            </h2>
                        </div>
                        <div class="body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                                    <thead>
                                        <tr>
                                            <th>Order ID</th>
                                            <th>User Name</th>
                                            <th>Company</th>
                                            <th>Model</th>
                                            <th>Engine Type</th>
                                            <th>Jobs</th>
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th>Order ID</th>
                                            <th>User Name</th>
                                            <th>Company</th>
                                            <th>Model</th>
                                            <th>Engine Type</th>
                                            <th>Jobs</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                        <% data.forEach(function(doc) {%>
                                        <tr>
                                            <td><%= doc._id%></td>
                                            <td><%= doc.userName%></td>
                                            <td><%= doc.company%></td>
                                            <td><%= doc.model%></td>
                                            <td><%= doc.engineType%></td>
                                            <td><%= doc.jobs%></td>
                                        </tr>  
                                        <% }) %>                   
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


    
    <%- include('./partials/bottom')%>